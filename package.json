{
  "name": "@trieb.work/zoho-ts",
  "version": "0.2.1",
  "description": "Our Typescript library helps you to work easily with the Zoho Inventory API. Most functions are generic and self-explanatory. Some are specific to our use-cases",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/trieb-work/zoho-ts.git"
  },
  "author": "Jannik Zinkl | trieb.work",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && yarn tsc",
    "clean": "yarn rimraf dist",
    "publish": "yarn run clean && yarn run build && yarn npm publish --access public",
    "dev": "yarn tsc --watch --preserveWatchOutput",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js --fix"
  },
  "dependencies": {
    "async-retry": "^1.3.1",
    "await-timeout": "^1.1.1",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "dayjs": "^1.10.4",
    "form-data": "^3.0.0",
    "simple-oauth2": "^4.2.0",
    "ts-mixer": "^5.4.0"
  },
  "devDependencies": {
    "@trieb.work/eslint-config-base": "^1.6.7",
    "@trieb.work/prettier-base": "^0.1.0-alpha.2",
    "@trieb.work/tsconfig-base": "^1.6.1",
    "@types/assert": "^1.5.2",
    "@types/async-retry": "^1.4.2",
    "@types/await-timeout": "^0.3.1",
    "@types/node": "^14.14.19",
    "@types/rimraf": "^3",
    "@types/simple-oauth2": "^4.1.0",
    "@typescript-eslint/parser": "^4.11.1",
    "assert": "^2.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.31.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.3.5"
  }
}
